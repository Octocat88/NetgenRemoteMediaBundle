{% block ngremotemedia_field %}
    <img src="{{ field.value.secure_url }}"
            {% if field.value.width is defined %} width="{{ field.value.width }}"{% endif %}
            {% if field.value.height is defined %} height="{{ field.value.height }}"{% endif %}
         {#alt="{{ field.value.alternativeText }}"#} />
{% endblock %}
